  "engines": {
    "node": ">= 14.15.0"
  },
  "scripts": {
    "tsc": "tsc",
    "clean": "rimraf dist",
    "build": "yarn clean && tsc",
    "start": "yarn build && NODE_ENV=production node dist/src/server.js",
    "dev": "NODE_ENV=development nodemon dist/src/server.js",
    "make:migration": "knex migrate:make",
    "migrate:prod": "NODE_ENV=production knex migrate:latest",
    "migrate": "NODE_ENV=development knex migrate:latest",
    "rollback:prod": "NODE_ENV=production knex migrate:rollback",
    "rollback": "NODE_ENV=development knex migrate:rollback",
    "test": "NODE_ENV=test jest --watchAll --no-cache",
    "lint": "eslint --fix ./src --ext .js,.ts"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cookie-session": "^2.0.42",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.19",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.14",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.2.0",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.13",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  }
